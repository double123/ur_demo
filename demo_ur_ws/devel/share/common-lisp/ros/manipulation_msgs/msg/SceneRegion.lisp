(cl:defmethod roslisp-msg-protocol:serialization-length ((msg <SceneRegion>))
  (cl:+ 0
     (roslisp-msg-protocol:serialization-length (cl:slot-value msg 'cloud))
     4 (cl:reduce #'cl:+ (cl:slot-value msg 'mask) :key #'(cl:lambda (ele) (cl:declare (cl:ignorable ele)) (cl:+ 4)))
     (roslisp-msg-protocol:serialization-length (cl:slot-value msg 'image))
     (roslisp-msg-protocol:serialization-length (cl:slot-value msg 'disparity_image))
     (roslisp-msg-protocol:serialization-length (cl:slot-value msg 'cam_info))
     (roslisp-msg-protocol:serialization-length (cl:slot-value msg 'roi_box_pose))
     (roslisp-msg-protocol:serialization-length (cl:slot-value msg 'roi_box_dims))
))
(cl:defmethod roslisp-msg-protocol:ros-message-to-list ((msg <SceneRegion>))
  "Converts a ROS message object to a list"
  (cl:list 'SceneRegion
    (cl:cons ':cloud (cloud msg))
    (cl:cons ':mask (mask msg))
    (cl:cons ':image (image msg))
    (cl:cons ':disparity_image (disparity_image msg))
    (cl:cons ':cam_info (cam_info msg))
    (cl:cons ':roi_box_pose (roi_box_pose msg))
    (cl:cons ':roi_box_dims (roi_box_dims msg))
))
